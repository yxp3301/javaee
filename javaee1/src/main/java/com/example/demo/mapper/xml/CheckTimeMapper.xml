<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CheckTimeMapper">
    <resultMap id="CheckTimeResultMap" type="com.example.demo.entity.CheckTime">
        <id property="checkTimeID" column="check_timeID" />
        <result property="startTime" column="start_time" />
        <result property="endTime" column="end_time" />
        <result property="studentCapacity" column="student_capacity" />
        <result property="classID" column="classID" />
    </resultMap>
    <select id="findCheckTimeByID" resultMap="CheckTimeResultMap">
        SELECT * FROM check_time WHERE check_timeID = #{checkTimeID}
    </select>
</mapper>